{
  "name": "flow",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "postinstall": "cd client && npm install"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.19.2"
  },
  "proxy": "http://localhost:3005"
}
